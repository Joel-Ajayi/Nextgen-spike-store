{
  "name": "flipkart-backend",
  "version": "1.0.0",
  "description": "backend of flipkart project by Ayotunde",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --transpile-only --no-notify src/index.ts",
    "dev:typecheck": "tsc --noEmit --watch",
    "postinstall": "npm -s run generate",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate --schema=src/db/prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only  src/servers/graphql/schema/index.ts --nexusTypegen"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.15.0",
    "apollo-server-express": "^3.8.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "graphql-upload": "^15.0.1",
    "mongoose": "^6.3.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nexus": "^1.3.0",
    "redis": "^4.1.0",
    "slugify": "^1.6.5",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.40",
    "nodemon": "^2.0.16",
    "prisma": "^3.15.0",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0"
  }
}
